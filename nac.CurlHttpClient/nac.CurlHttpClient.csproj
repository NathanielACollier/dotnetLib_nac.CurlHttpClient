<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageVersion>1.0.5</PackageVersion>
    <Description>Provides a high level HttpClient with json support from NewtonSoft.Json</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/NathanielACollier/dotnetLib_nac.CurlHttpClient</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="nac.CurlHttpClient.LowLevel" Version="1.0.4" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
